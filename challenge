pragma solidity 0.8.9;

contract state {
    uint  value;
    bool check;
    int num;
    string word;
    function setValue() public
    {
        value=9;
    }
    function getValue() public view returns(uint)
    {
        return value;
    }
    function setCheck() public{
        check;
    }
    function getCheck() public view returns(bool){
        return check;
    }
    function setNum() public{
        num=8;
    }
    function getNum() public view returns(int){
        return num;
    }
    function setWord() public{
        word="hello";
    }
    function getWord() public view returns(string memory){
        return word;
    }
}
